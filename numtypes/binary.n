class Bit [value:int] {
	let internal = value % 2
	let pub toString = [] -> str {
		return intInBase10(internal)
	}

	let pub toInt = [] -> int {
		return internal
	}
}

class Byte [value:list[Bit]] {
	let pub toString = [] -> str {
		let out = ""
		for (bit in value) {
			var out = out + bit.toString()
		}
		return out
	}


	let pub toInt = [] -> int {
		let out = 0
		return 0
	}
}

