class Bit [value:int] {
	let internal = value % 2
	let pub toString = [] -> str {
		return intInBase10(internal)
	}

	let pub toInt = [] -> int {
		return internal
	}
}

class Byte [value:list[Bit]] {
	let pub toString = [] -> str {
		let out = ""
		value
			|> filterMap(([thing: Bit] -> maybe[str] {
					var out = out + thing.toString()
				 	return yes(thing.toString())
				 }))
		return out
	}

	// TODO: to int
}

